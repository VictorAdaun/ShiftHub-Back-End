{
  "name": "ezscheduler",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon",
    "dev:start:public": "ts-node ./src/server/public.ts",
    "dev:start:internal": "ts-node ./src/server/internal.ts",
    "dev:concurrently": "concurrently yarn:dev:start:*",
    "env:development": "ln -sf .env.local .env",
    "env:staging": "ln -sf .env.staging .env",
    "env:staging-oms": "ln -sf .env.oms .env",
    "env:test": "ln -sf .env.test .env",
    "env:production": "ln -sf .env.prod .env",
    "build": "rm -rf ./build && prisma generate && tsc",
    "start:public": "node ./build/server/public.js",
    "start:internal": "node ./build/server/internal.js",
    "migrate:dev": "prisma migrate dev",
    "jest": "NODE_ENV=test LOG_LEVEL=error jest",
    "test": "NODE_ENV=test LOG_LEVEL=error jest"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/nanoid-dictionary": "^4.2.0",
    "@types/node": "^20.6.1",
    "@types/node-uuid": "^0.0.29",
    "@types/swagger-ui-express": "^4.1.3",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@prisma/client": "^5.3.1",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "class-validator-jsonschema": "^5.0.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "deepmerge": "^4.3.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "google-auth-library": "^9.0.0",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.0.1",
    "nanoid-dictionary": "^4.3.0",
    "node": "^20.6.0",
    "node-uuid": "^1.4.8",
    "prisma": "^5.3.1",
    "routing-controllers": "^0.10.4",
    "routing-controllers-openapi": "^4.0.0",
    "swagger-ui-express": "^5.0.0",
    "typedi": "^0.10.0",
    "winston": "^3.10.0"
  }
}
